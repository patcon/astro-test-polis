---
import BaseLayout from '../layouts/BaseLayout.astro';
import TypeformEmbed from '../components/TypeformEmbed.astro';
import '../styles/typeform.css';
const pageTitle = 'Pre-Survey';
const typeformId = "eVNMLfqi";
---
<BaseLayout pageTitle={ pageTitle }>
    <script is:inline>
        const isDoneSurvey = localStorage.getItem('isDoneSurvey');
        if (isDoneSurvey) {
            window.location.replace("/participate")
        }
    </script>
    <TypeformEmbed formId={typeformId} enableSandbox />
</BaseLayout>